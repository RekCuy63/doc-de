<refentry xml:id="function.tanh" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>tanh</refname>
  <refpurpose>Tangens hyperbolikus</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>float</type><methodname>tanh</methodname>
   <methodparam><type>float</type><parameter>num</parameter></methodparam>
  </methodsynopsis>
  <para>
   Berechnet den Tangens hyperbolikus des Parameters
   <parameter>num</parameter>, welcher als
   <literal>sinh($num)/cosh($num)</literal> definiert ist.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>num</parameter></term>
     <listitem>
      <para>
       Der zu verarbeitende Wert.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Der Tangens hyperbolikus von <parameter>num</parameter>.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>tan</function></member>
    <member><function>atanh</function></member>
    <member><function>sinh</function></member>
    <member><function>cosh</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>
